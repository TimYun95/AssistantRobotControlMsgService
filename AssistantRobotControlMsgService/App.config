<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <appSettings>
    <!--本机用户-->
    <add key="userName" value="admin"/>
    <add key="userPWD" value="admin"/>
    <add key="userSession" value="1"/>
    <!--网段配置-->
    <add key="ifAtSamePC" value="False"/>
    <!--服务器IP地址配置-->
    <add key="serverIPAtDiffPC" value="192.168.1.13"/>
    <!--TCP时间配置-->
    <add key="tcpSocketRecieveTimeOut" value="4000"/>
    <add key="tcpSocketSendTimeOut" value="1000"/>
    <!--启动子程序路径配置-->
    <add key="localUIProgramName" value="AssistantRobot.exe"/>
    <add key="localUIProgramPath" value="D:\AssistantRobot\LocalUI\"/>
    <!--缓冲区大小配置-->
    <add key="maxBufferSize" value="20"/>
    <!--队列间隔时间配置-->
    <add key="waitTimerMsForBuffer" value="30"/>
  </appSettings>
  
  
  
  
</configuration>